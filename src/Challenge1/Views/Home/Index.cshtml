@model Challenge1.ViewModels.HomeModel;
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Oppgave 1</h1>
</div>

<div>
    <h1>Kundeliste:</h1>
    <ul>
        @foreach (var record in Model.Customers)
        {
            <li>@record.Id: @record.FirstName @record.LastName</li>
        }
    </ul>
</div>
